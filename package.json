{
  "name": "doom-but-typescript-types",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "playground": "pnpm --filter playground run playground",
    "generate": "tsx ./packages/playground/generate.ts",
    "test:conformance": "pnpm --filter conformance-tests run test",
    "test:math": "pnpm --filter ts-type-math run test",
    "test:create-new": "pnpm --filter conformance-tests run test:create-new",
    "test:rust": "cargo test",
    "test": "pnpm run test:rust && CI=true pnpm run test:math && CI=true pnpm run test:conformance && pnpm run build",
    "build": "tsc",
    "build:force": "MICHIGAN_TYPESCRIPT=true time tsc --project tsconfig.force.json"
  },
  "dependencies": {
    "@biomejs/js-api": "0.3.0-nightly.cfef948",
    "@biomejs/wasm-bundler": "^1.5.3",
    "@biomejs/wasm-nodejs": "^1.5.3",
    "@biomejs/wasm-web": "^1.5.3",
    "@types/node": "^20.11.5",
    "@typescript/vfs": "^1.5.0",
    "lz-string": "^1.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "type-testing": "^0.2.0",
    "typescript": "5.3.3",
    "vitest": "^1.2.1",
    "wabt": "^1.0.32"
  },
  "pnpm": {
    "patchedDependencies": {
      "typescript@5.3.3": "patches/typescript@5.3.3.patch",
      "@biomejs/js-api@0.3.0-nightly.cfef948": "patches/@biomejs__js-api@0.3.0-nightly.cfef948.patch",
      "type-testing@0.2.0": "patches/type-testing@0.2.0.patch"
    }
  }
}

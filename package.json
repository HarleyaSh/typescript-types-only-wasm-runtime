{
  "name": "doom-but-typescript-types",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "playground": "pnpm --filter playground run playground",
    "generate": "tsx ./packages/playground/generation.ts",
    "test:conformance": "pnpm --filter conformance-tests run test",
    "test:math": "pnpm --filter ts-type-math run test",
    "test:create-new": "pnpm --filter conformance-tests run test:create-new",
    "test:rust": "cargo test",
    "test": "pnpm run test:rust && CI=true pnpm run test:math && CI=true pnpm run test:conformance && pnpm run build",
    "build": "tsc"
  },
  "dependencies": {
    "@types/node": "^20.11.5",
    "@types/ramda": "^0.29.10",
    "@typescript/vfs": "^1.5.0",
    "lz-string": "^1.5.0",
    "ramda": "0.29.1",
    "ts-arithmetic": "^0.1.1",
    "ts-node": "^10.9.2",
    "ts-toolbelt": "^9.6.0",
    "tsx": "^4.7.0",
    "type-testing": "^0.2.0",
    "typescript": "5.3.3",
    "vitest": "^1.2.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "typescript@5.3.3": "patches/typescript@5.3.3.patch"
    }
  }
}
